 SELECT DISTINCT L.PAGE_ID AS RECOMMENDED_PAGE
    FROM LIKES L
    WHERE L.USER_ID IN(
        SELECT FI.USER2_ID
        FROM FRIENDSHIP FI
        WHERE FI.USER1_ID  = 1
        UNION
        SELECT FI.USER1_ID
        FROM FRIENDSHIP FI
        WHERE FI.USER2_ID  = 1
    ) 
    AND PAGE_ID NOT IN(
    SELECT PAGE_ID
    FROM LIKES
    WHERE USER_ID = 1
    )
